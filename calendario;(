	$('.ui-corner-all').click(function(){
		day();
	})	


function day() {
	var currentDay = $('.ui-state-active').text();
	var currentM = $('.ui-state-active').parent('td').attr('data-month');
	var currentY = $('.ui-state-active').parent('td').attr('data-year');
	$('.current-day').children('h3').text(currentDay +" / "+currentM+" / "+currentY);
	$('td').on('click', function () {
		$('.current-day').children('h3').hide().text(currentDay).fadeIn(400);
		console.log(currentDay%7)
		day();
	});	
	$
}